  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Run Chrome",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src"
    },
    {
      "name": "SWA: Run Online-Lesson",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:4280",
      "preLaunchTask": "swa: start Online-Lesson",
      "webRoot": "${workspaceFolder}/",
      "timeout": 30000
    }
  ]
}
